                                      <b:if cond='data:i.link'>
                                        <a expr:href='data:i.link'><data:i.title/></a>
                                        <b:else/>
                                        <b:if cond='data:i.url'>
                                          <b:if cond='data:blog.url != data:post.url'>
                                            <a expr:href='data:i.url'><data:i.title/></a>
                                            <b:else/>
                                            <data:i.title/>
                                          </b:if>
                                          <b:else/>
                                          <data:i.title/>
                                        </b:if>
                                      </b:if>
                                    </h2>
                                  </div>
